{
  "version": "2.0",
  "rules": [
    {
      "origin": "ajax.googleapis.com",
      "target": "ajax.loli.net",
      "enable": true
    },
    {
      "origin": "fonts.googleapis.com",
      "target": "fonts.googleapis.cn",
      "enable": true
    },
    {
      "origin": "fonts.gstatic.com",
      "target": "fonts.googleapis.cn",
      "enable": true
    },
    {
      "origin": "themes.googleusercontent.com",
      "target": "themes.loli.net",
      "enable": true
    },
    {
      "origin": "secure.gravatar.com",
      "target": "gravatar.loli.net",
      "enable": true
    },
    {
      "origin": "www.gstatic.com",
      "target": "www.gstatic.cn",
      "enable": true
    },
    {
      "origin": "developers.google.com",
      "target": "developers.google.cn",
      "enable": true
    },
    {
      "origin": "developer.android.com",
      "target": "developer.android.google.cn",
      "enable": true
    },
    {
      "origin": "source.android.com",
      "target": "source.android.google.cn",
      "enable": true
    },
    {
      "origin": "www.google.com/recaptcha",
      "target": "www.recaptcha.net/recaptcha",
      "enable": true
    },
    {
      "origin": "cdnjs.cloudflare.com",
      "target": "s4.zstatic.net",
      "enable": true
    },
    {
      "origin": "unpkg.com/(\\w+)@([0-9.]+)/",
      "target": "registry.npmmirror.com/$1/$2/files/",
      "enable": true
    }
  ]
}
